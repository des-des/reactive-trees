{
  "name": "es6-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for running transpiled tests fast",
  "main": "lib/app.js",
  "dependencies": {
    "d3": "^3.5.16",
    "hapi": "^13.0.0",
    "inert": "^3.2.0",
    "jquery": "^2.2.1",
    "rx": "^4.0.8"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-istanbul": "^0.6.0",
    "babel-preset-es2015": "^6.5.0",
    "socket.io": "^1.4.5",
    "tap-notify": "0.0.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0"
  },
  "scripts": {
    "test": "node test",
    "dev:test": "nodemon -q --exec \"node test/ | tap-notify | tap-spec\"",
